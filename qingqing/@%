#include "stdio.h"

int a[5] = {1,2,3,4,0};
int b[5] = {1,2,3,5,0};

int main()
{
    int j;
   // printf("%d\r\n",sizeof(a));
    for (int i = 0; a[i] != 0;i++) {
        for ( j = 0; a[j]!= 0;j++ ) {
            if (a[i] == b[j]) {
                continue;
            }
        }
        if (b[j]==0) {
            printf("%d\r\n",a[i]);
            break;
        }
    }
}
